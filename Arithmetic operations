/*Arithmetic operations*/
public class Main
{
public static void main(String[] args)
{
   int x=6;
   int y=3;
   int A=x+y,S=x-y,M=x*y,D=x/y;
System.out.println("Result of Addition       "+x+" and "+y+" is "+A);
System.out.println("Result of Subtraction    "+x+" and "+y+" is "+S);
System.out.println("Result of Multiplication "+x+" and "+y+" is "+M);
System.out.println("Result of Division       "+x+" and "+y+" is "+D);
}

}

