import java.io.*;
import java.util.*;
class Circle{
    double r;
    double area(){
        return(22/7.0)*r*r;
    }
}
 public class CircleMain{
    public static void main(String args[]){
        Circle c=new Circle();
        c.r=5.5;
        System.out.println("Area of circle with radius " +c.r+ " is: " +c.area());
    }
}
